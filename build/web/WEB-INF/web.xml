<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostQuestion</servlet-name>
        <servlet-class>PostQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostReply</servlet-name>
        <servlet-class>PostReply</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeneralQs</servlet-name>
        <servlet-class>GeneralQs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeneralReplies</servlet-name>
        <servlet-class>GeneralReplies</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>removeUser</servlet-name>
        <servlet-class>removeUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>unblockUser</servlet-name>
        <servlet-class>unblockUser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostQuestion</servlet-name>
        <url-pattern>/PostQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostReply</servlet-name>
        <url-pattern>/PostReply</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeneralQs</servlet-name>
        <url-pattern>/GeneralQs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeneralReplies</servlet-name>
        <url-pattern>/GeneralReplies</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>removeUser</servlet-name>
        <url-pattern>/removeUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>unblockUser</servlet-name>
        <url-pattern>/unblockUser</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
